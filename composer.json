{
  "name": "setbased/exception",
  "description": "Exceptions with format string",
  "keywords": [
    "Exception",
    "Format"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.4"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phing/phing": "^3.0.0-RC4",
    "phpunit/phpunit": "^9.5.0"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Exception\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Exception\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
