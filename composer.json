{
  "name": "setbased/exception",
  "description": "Exceptions with format string",
  "keywords": [
    "Exception",
    "Format"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "phing/phing": "^2.16.3",
    "phpunit/phpunit": "^9.5.0"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Exception\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Exception\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
